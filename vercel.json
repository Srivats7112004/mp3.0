{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze-image.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "explore.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "vendors.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/analyze-image",
      "dest": "/api/analyze-image.py",
      "methods": ["POST"]
    },
    {
      "src": "/explore-styles.css",
      "dest": "/explore-styles.css"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/explore-script.js",
      "dest": "/explore-script.js"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/vendors.json",
      "dest": "/vendors.json"
    },
    {
      "src": "/explore",
      "dest": "/explore.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

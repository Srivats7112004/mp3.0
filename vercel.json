{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze-image.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/analyze-image",
      "dest": "/api/analyze-image.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
